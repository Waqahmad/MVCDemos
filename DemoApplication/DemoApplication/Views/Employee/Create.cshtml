@using DemoApplication.Models
@model Employee
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm())
{
	<table>
		<tr>
			<th>
				Name
			</th>
			<td>
				@Html.EditorFor(p=>p.Name)
			</td>
		</tr>

		<tr>
			<th>
				Department ID
			</th>
			<td>
				@Html.EditorFor(x=>x.DepartmentID)

			</td>
		</tr>

		<tr>
			<th>
				Age
			</th>
			<td>
				@Html.EditorFor(x=>x.Age)

			</td>
		</tr>
		<tr>
			<td>

			</td>
			<td>
			<input type="submit" value="Save" />
			</td>
		</tr>

	</table>


}

